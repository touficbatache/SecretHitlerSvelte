{
  "name": "secrethitler",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "deploy:dev": "firebase use dev && firebase deploy",
    "deploy:prod": "firebase use production && firebase deploy",
    "link": "cp example.firebaserc .firebaserc && cp functions/example.env functions/.env.production && cp functions/example.env functions/.env.dev && cp functions/example.env functions/.env.local && ed -s .firebaserc <<ED_COMMANDS\n,s/FIREBASEPROJECT/${npm_config_project}/g\nw\nED_COMMANDS"
  },
  "dependencies": {
    "@auditors/core": "^0.7.0",
    "@fontsource-variable/nunito": "^5.0.17",
    "@fontsource/courier-prime": "^5.0.8",
    "@iconify/svelte": "^3.1.4",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@theatre/core": "^0.7.0",
    "@theatre/studio": "^0.7.0",
    "@threlte/core": "^7.0.12",
    "@threlte/extras": "^8.1.1",
    "@threlte/theatre": "^2.1.5",
    "@types/three": "^0.160.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.3.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "firebase": "^10.4.0",
    "firebase-admin": "^11.10.1",
    "firebase-frameworks": "^0.11.2",
    "iconify-icon": "^1.0.8",
    "postcss": "^8.4.31",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "sveltekit-sse": "^0.3.3",
    "tailwindcss": "^3.3.5",
    "three": "^0.160.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "uuid": "^9.0.0",
    "vite": "^4.4.2"
  },
  "type": "module",
  "devDependencies": {
    "eslint-plugin-import": "^2.29.1"
  }
}
